<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>EDDC_EADS2.ac</path>

<animation>
 <type>material</type>
    <object-name>eads2rollup</object-name>
    <object-name>eads2winbar</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.6</red>	
  <green>0.6</green>	
  <blue>0.6</blue>
 </emission>
</animation>

