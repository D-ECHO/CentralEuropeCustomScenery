<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>EDDC_EADS1.ac</path>

<animation>
 <type>material</type>
    <object-name>redlight</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>1.0</red>	
  <green>1.0</green>	
  <blue>1.0</blue>
 </emission>
</animation>

