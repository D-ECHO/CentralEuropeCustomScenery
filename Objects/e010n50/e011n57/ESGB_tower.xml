<PropertyList>

 <path>ESGB_tower.ac</path>

 <animation>
  <type>select</type>
  <condition>
   <greater-than>
    <property>/sim/time/utc/year</property>
    <value>1968</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>30000</max-m>
 </animation>

</PropertyList>
