<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>
	<path>LKPR_NA_TRAVELSERV.ac</path>
<animation>
<type>material</type>
    <object-name>LKPR_NA_TRAVELSERV2</object-name>
<condition>		
<greater-than>		
	<property>/sim/time/sun-angle-rad</property>
	<value>1.57</value>
</greater-than>
</condition>
<emission>	
	<red>1</red>	
	<green>1</green>	
	<blue>1</blue>
</emission>
</animation>
<!-- Range  -->
<animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/detailed</max-property>
</animation>
</PropertyList>
