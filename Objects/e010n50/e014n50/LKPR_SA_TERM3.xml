<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>
	<path>LKPR_SA_TERM3.ac</path>
	
<!--   REMBRANDT model-transparent for the banister -->
<effect>
 <inherits-from>Effects/model-transparent</inherits-from>
 <condition><property>sim/rendering/rembrandt/enabled</property></condition>
 <object-name>LKPR_SA_TERM33</object-name>
</effect>
<animation>
<type>material</type>
    <object-name>LKPR_SA_TERM32</object-name>
<condition>		
<greater-than>		
	<property>/sim/time/sun-angle-rad</property>
	<value>1.57</value>
</greater-than>
</condition>
<emission>	
	<red>0.4</red>	
	<green>0.4</green>	
	<blue>0.4</blue>
</emission>
</animation>
<!-- Range  -->
<animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
</animation>
