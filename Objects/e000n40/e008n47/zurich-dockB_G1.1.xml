<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

  <path>zurich-dockB_G1.1.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Fenster1</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission>
      <red> 0.8 </red>
      <green> 0.8 </green>
      <blue> 0.3 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
