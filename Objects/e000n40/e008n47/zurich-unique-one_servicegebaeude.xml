   <animation>
    <type>material</type>
    <object-name>Fenster1</object-name>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission>
      <red> 0.5 </red>
      <green> 0.5 </green>
      <blue> 0.3 </blue>
    </emission>
  </animation>

