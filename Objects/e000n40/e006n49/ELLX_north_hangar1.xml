	  <max-m>15000</max-m>
    </animation>

<animation>
	<type>material</type> 
	<object-name>WINDOW</object-name>
	<condition>		
		<greater-than>		
			<property>/sim/time/sun-angle-rad</property>
			<value>1.57</value>
		</greater-than>
	</condition>
	<emission>	
		<red>.75</red>	
		<green>0.675</green>	
