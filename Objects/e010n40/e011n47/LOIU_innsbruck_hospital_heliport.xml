	<!-- Texture illuminate for helipad lights and obstruction lights glass-->
	<animation>
		<name>helilights_ilu</name>
		<object-name>floodlight_lamp</object-name>
		<object-name>osbt_lamp_glass</object-name>
	</animation>
	
	<animation>
	<type>material</type> 
	<object-name>helilights_ilu</object-name>
		<condition>		
			<greater-than>		
			<property>/sim/time/sun-angle-rad</property>
			<value>1.57</value>
			</greater-than>
		</condition>
		<emission>	
			<red>1.0</red>	
			<green>1.0</green>	
			<blue>1.0</blue>
			<!-- <factor-prop>/scenery/LOWI/LOJO_oamtc_helicopter_base[0]/light[0]</factor-prop> -->
		</emission>	
	</animation>
	
	<!-- Texture illuminate for helipad surface -->
	<animation>
		<name>helipad_ilu</name>
		<object-name>mainpad_01</object-name>
		<object-name>mainpad_02</object-name>
		<object-name>mainpad_03</object-name>
	</animation>
	
	<animation>
	<type>material</type> 
	<object-name>helipad_ilu</object-name>
		<condition>		
			<greater-than>		
			<property>/sim/time/sun-angle-rad</property>
			<value>1.57</value>
			</greater-than>
		</condition>
		<emission>	
			<red>0.3</red>	
			<green>0.3</green>	
			<blue>0.3</blue>
			<!-- <factor-prop>/scenery/LOWI/LOJO_oamtc_helicopter_base[0]/light[0]</factor-prop> -->
		</emission>
	</animation>
	
