<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>

    <path>Brake_MerchandStore.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>

  <animation>
    <type>material</type>

    <object-name>Mesh</object-name>
    <object-name>Mesh.001</object-name>
    <object-name>Cylinder.008</object-name>
    <object-name>Cylinder.007</object-name>
    <object-name>Cylinder.006</object-name>
    <object-name>Cylinder.004</object-name>
    <object-name>Tube.007</object-name>
    <object-name>Cube.002</object-name>
    <object-name>Cube.004</object-name>
    <object-name>Cube.003</object-name>
    <object-name>Cylinder.001</object-name>
    <object-name>Cube.005_1</object-name>
    <object-name>Mesh_1</object-name>
    <object-name>Cube</object-name>
    <object-name>Mesh.001_1</object-name>

    <object-name>Cylinder.002</object-name>
    <object-name>Cylinder.003</object-name>	
	
	    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission>
      <red>   0.3 </red>
      <green> 0.3 </green>
      <blue>  0.3 </blue>
    </emission>
    <spec>
      <red>   0.5 </red>
      <green> 0.5 </green>
      <blue>  0.5 </blue>
    </spec>
  </animation>

  <animation>
    <type>material</type>

    <object-name>Mesh</object-name>
    <object-name>Mesh.001</object-name>
    <object-name>Cylinder.008</object-name>
    <object-name>Cylinder.007</object-name>
    <object-name>Cylinder.006</object-name>
    <object-name>Cylinder.004</object-name>
    <object-name>Tube.007</object-name>
    <object-name>Cube.002</object-name>
    <object-name>Cube.004</object-name>
    <object-name>Cube.003</object-name>
    <object-name>Cylinder.001</object-name>
    <object-name>Cube.005_1</object-name>
    <object-name>Mesh_1</object-name>
    <object-name>Cube</object-name>
    <object-name>Mesh.001_1</object-name>

    <object-name>Cylinder.002</object-name>
    <object-name>Cylinder.003</object-name>
	
    <condition>
      <less-than-equals>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </less-than-equals>
    </condition>
    <emission>
      <red>   0.2 </red>
      <green> 0.2 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>

</PropertyList>

