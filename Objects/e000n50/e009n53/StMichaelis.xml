<?xml version="1.0"?>
<PropertyList>
  <path>StMichaelis.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>15000</max-m>
  </animation>

  <animation>
    <type>range</type>
    <object-name>Clocks</object-name>
    <min-m>0</min-m>
    <max-m>1000</max-m>
  </animation>

  <animation>
    <type>range</type>
    <object-name>Flag</object-name>
    <min-m>0</min-m>
    <max-m>2000</max-m>
  </animation>

  <animation>
    <type>range</type>
    <object-name>Stairs</object-name>
    <min-m>0</min-m>
    <max-m>500</max-m>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Flag</object-name>
    <property>/environment/wind-from-heading-deg</property>
    <factor>-1</factor>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
    <center>
      <x-m>-30.4404</x-m>
      <y-m>0</y-m>
      <z-m>142.577</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EastClockMinute</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>-0.1</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-30.4415</x-m>
      <y-m>0</y-m>
      <z-m>77.3483</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>EastClockHour</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>-8.33333e-3</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-30.4415</x-m>
      <y-m>0</y-m>
      <z-m>77.3483</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>WestClockMinute</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>0.1</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-30.4415</x-m>
      <y-m>0</y-m>
      <z-m>77.3483</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>WestClockHour</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>8.33333e-3</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-30.4415</x-m>
      <y-m>0</y-m>
      <z-m>77.3483</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>NorthClockMinute</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>-0.1</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-30.4415</x-m>
      <y-m>0</y-m>
      <z-m>77.3483</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>NorthClockHour</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>-8.33333e-3</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-30.4415</x-m>
      <y-m>0</y-m>
      <z-m>77.3483</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>SouthClockMinute</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>0.1</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-30.4415</x-m>
      <y-m>0</y-m>
      <z-m>77.3483</z-m>
    </center>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>SouthClockHour</object-name>
    <property>/sim/time/utc/day-seconds</property>
    <factor>8.33333e-3</factor>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>-30.4415</x-m>
      <y-m>0</y-m>
      <z-m>77.3483</z-m>
    </center>
  </animation>

</PropertyList>
