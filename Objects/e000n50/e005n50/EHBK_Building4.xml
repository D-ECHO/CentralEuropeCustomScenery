<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

<path>EHBK_Building4.ac</path>

<animation>
   <name>Building1</name>
<object-name>Roof_Cube.002</object-name>
<object-name>Stairs.001_Plane.002</object-name>
<object-name>Stairs_Plane.002</object-name>
<object-name>Util_Cube.001</object-name>
<object-name>Util_Cube.001_0</object-name>
<object-name>Util_Cube.001_1</object-name>
<object-name>fence_Cube.002</object-name>
</animation>

<animation>
    <type>textranslate</type> 
    <object-name>Building1</object-name> 
    <property>/sim/time/sun-angle-rad</property>
    <step> 1.57 </step>
    <factor>0.318471338</factor>
    <axis> 
      <x> -1 </x> 
      <y>  0 </y> 
    </axis> 
</animation>

<animation>
 <type>material</type> 
 <object-name>Building1</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.3</red>	
  <green>0.3</green>	
  <blue>0.3</blue>
 </emission>
</animation>

<animation>
 <type>material</type> 
 <object-name>Building1</object-name>
 <condition>		
  <less-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </less-than>
 </condition>
 <emission>	
  <red>0.3</red>	
  <green>0.3</green>	
  <blue>0.3</blue>
 </emission>
</animation>


<animation>
   <name>Lights</name>
<object-name>TL.001_Cube.001</object-name>
<object-name>TL_Cube.002</object-name>
</animation>

<animation>
 <type>material</type> 
 <object-name>Lights</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.7</red>	
  <green>0.7</green>	
  <blue>0.6</blue>
 </emission>
</animation>

</PropertyList>
