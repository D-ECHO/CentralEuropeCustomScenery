<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

<path>EHBK_Gates.ac</path>

<animation>
   <name>Building1</name>
<object-name>GatesEntrance_Cub.00</object-name>
<object-name>GatesRoof_Cube.001</object-name>
<object-name>Gates_Cube</object-name>
<object-name>Gates_Cube_0</object-name>
<object-name>Gates_Cube_1</object-name>
<object-name>SecTower_Circle</object-name>
</animation>

<animation>
    <type>textranslate</type> 
    <object-name>Building1</object-name> 
    <property>/sim/time/sun-angle-rad</property>
    <step> 1.57 </step>
    <factor>0.318471338</factor>
    <axis> 
      <x> 0 </x> 
      <y> -1 </y> 
    </axis> 
</animation>

<animation>
 <type>material</type> 
 <object-name>Building1</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.3</red>	
  <green>0.3</green>	
  <blue>0.3</blue>
 </emission>
</animation>

<animation>
 <type>material</type> 
 <object-name>Building1</object-name>
 <condition>		
  <less-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </less-than>
 </condition>
 <emission>	
  <red>0.3</red>	
  <green>0.3</green>	
  <blue>0.3</blue>
 </emission>
</animation>

<animation>
   <name>Text</name>
<object-name>Arrival_Plane.001</object-name>
<object-name>Depart_Plane</object-name>
</animation>

<animation>
 <type>material</type> 
 <object-name>Text</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>0.6</red>	
  <green>0.6</green>	
  <blue>0.5</blue>
 </emission>
</animation>

<animation>
 <type>material</type> 
 <object-name>Text</object-name>
 <condition>		
  <less-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </less-than>
 </condition>
 <emission>	
  <red>0.3</red>	
  <green>0.3</green>	
  <blue>0.3</blue>
 </emission>
</animation>

</PropertyList>
